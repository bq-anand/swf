{
  "isDebug": true,
  "logger": {
    "transports": {
      "Console": {
        "silent": false,
        "level": "info",
        "handleExceptions": false,
        "prettyPrint": true,
        "colorize": true,
        "showLevel": false
      }
    }
  },
  "knex": {

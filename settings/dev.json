  "swf": {
    "accessKeyId": "Define this key in dev.specific.json or dev.local.json",
    "secretAccessKey": "Define this key in dev.specific.json or dev.local.json",
    "region": "Define this key in dev.specific.json or dev.local.json"
  }
}
